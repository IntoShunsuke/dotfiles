# aliases
alias ls="ls -F --color=auto"
alias ll="ls -l"
alias la="ls -a"

alias grep='grep --color=auto'
alias c="clear"
alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'

alias python="python3"
alias vim="nvim"

alias sc="screen"
alias scl="screen -ls"
alias scs="screen -S"
alias scr="screen -r"




